<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="252.0" prefWidth="485.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.skyfoxdigital.mepos.MainController">
   <children>
      <RadioButton fx:id="radUSB" layoutX="24.0" layoutY="130.0" mnemonicParsing="false" onAction="#usbSelected" selected="true" text="USB" />
      <RadioButton fx:id="radWiFi" layoutX="91.0" layoutY="130.0" mnemonicParsing="false" onAction="#wifiSelected" text="WiFi" />
      <TextField fx:id="txtIP" disable="true" layoutX="159.0" layoutY="126.0" opacity="0.4" prefHeight="25.0" prefWidth="114.0" promptText="MePOS IP" />
      <Button layoutX="365.0" layoutY="126.0" mnemonicParsing="false" onAction="#createInstance" text="Create instance" />
      <Button fx:id="btnPrint" disable="true" layoutX="64.0" layoutY="174.0" mnemonicParsing="false" onAction="#printReciept" prefHeight="39.0" prefWidth="142.0" text="Print test receipt">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnOpen" disable="true" layoutX="273.0" layoutY="174.0" mnemonicParsing="false" onAction="#openCashDrawer" prefHeight="39.0" prefWidth="142.0" text="Open cash drawer">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="22.0" layoutY="21.0" prefHeight="56.0" prefWidth="441.0" text="MePOS Mobile" textAlignment="CENTER" textFill="#00afe4">
         <font>
            <Font name="Open Sans" size="58.0" />
         </font>
      </Label>
      <Label fx:id="lblStatus" alignment="CENTER_RIGHT" layoutX="17.0" layoutY="230.0" prefHeight="17.0" prefWidth="452.0" text="Waiting for instance creation...">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
